doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Document
    body 
        h1 USER LIST
        each user in users
            p Name: #{user.name}
            ul 
                li Username: #{user.username}
                li Age: #{user.age}
                li Email: #{user.email}
                